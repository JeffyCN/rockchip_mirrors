#include "vfat.config"
#include "ntfs.config"

BR2_PACKAGE_BUSYBOX_CONFIG_FRAGMENT_FILES+="board/rockchip/yx/busybox.fragment"
BR2_PACKAGE_HOST_LINUX_HEADERS_CUSTOM_4_19=y
BR2_TARGET_GENERIC_ISSUE=""

# systemd
BR2_INIT_SYSTEMD=y
BR2_PACKAGE_SYSTEMD_ANALYZE=y
BR2_PACKAGE_SYSTEMD_FIRSTBOOT=y
BR2_SYSTEM_DEFAULT_PATH="/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin"

# timezone
BR2_TARGET_LOCALTIME="Asia/Shanghai"

# gdb
BR2_GDB_VERSION_11=y
BR2_PACKAGE_GDB=y
BR2_PACKAGE_GDB_SERVER=y
BR2_PACKAGE_GDB_DEBUGGER=y
BR2_PACKAGE_HOST_GDB=y
BR2_PACKAGE_HOST_GDB_TUI=y
BR2_PACKAGE_HOST_GDB_PYTHON=y

# network
